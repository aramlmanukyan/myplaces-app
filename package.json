{
  "name": "my-places",
  "version": "1.0.0",
  "description": "The great app to share interestign outdoor places",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=prod|| set NODE_ENV=prod&& pm2 start ./index.js --name myplaces",
    "start_test": "export NODE_ENV=dev|| set NODE_ENV=dev&& pm2 start ./index.js --name myplaces_test && pm2 logs",
    "restart": "pm2 restart myplaces && pm2 logs",
    "restart_test": "pm2 restart myplaces_test && pm2 logs",
    "stop": "pm2 stop myplaces",
    "stop_test": "pm2 stop myplaces_test",
    "test": "NODE_ENV=dev mocha  --recursive --reporter spec -require babel-register test"
  },
  "author": "Aram Manukyan",
  "license": "ISC",
